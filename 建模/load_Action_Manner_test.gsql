DROP JOB load_Action_Manner_test
CREATE ONLINE_POST JOB load_Action_Manner_test FOR GRAPH c_graph{
	DEFINE HEADER Action_Manner_header = "act","obj","rel","qa";
	LOAD
		TO VERTEX object VALUES ($"obj", $"obj"),
		TO VERTEX action VALUES ($"act", $"act"),
		TO VERTEX ANS VALUES ($"qa", $"qa"),

		TO EDGE obj_to_answer VALUES ($"obj",$"qa"),
		TO EDGE act_to_answer VALUES ($"act",$"qa"),

		TO EDGE Action_Manner VALUES ($"act",$"qa"),
		TO EDGE re_Action_Manner VALUES ($"obj",$"qa")

        USING QUOTE="double",USER_DEFINED_HEADER="Action_Manner_header";

# RUN JOB load_Action_Manner_test USING FILENAME="XXX", SEPARATOR=" ", EOL="\n"
}
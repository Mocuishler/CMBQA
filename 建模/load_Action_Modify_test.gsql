DROP JOB load_Action_Modify_test
CREATE ONLINE_POST JOB load_Action_Modify_test FOR GRAPH c_graph{
	DEFINE HEADER Action_Modify_header = "act","obj","rel","qa";
	LOAD

		TO VERTEX object VALUES ($"obj", $"obj"),
		TO VERTEX action VALUES ($"act", $"act"),
		TO VERTEX ANS VALUES ($"qa", $"qa"),

		TO EDGE obj_to_answer VALUES ($"obj",$"qa"),
		TO EDGE act_to_answer VALUES ($"act",$"qa"),

		TO EDGE passivity VALUES ($"obj",$"qa"),

		TO EDGE Action_Modify VALUES ($"act",$"obj")

		USING QUOTE="double",USER_DEFINED_HEADER="Action_Modify_header";

# RUN JOB load_Action_Modify_test USING FILENAME="XXX", SEPARATOR=" ", EOL="\n"
}